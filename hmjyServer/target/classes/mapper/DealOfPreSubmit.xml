<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.hmjy.ljq.mapper.DealOfPreSubmitMapper">
    <select id="getTeaInfo" resultType="java.util.Map">
        SELECT a.name,a.price,a.stores from tea_list a WHERE a.id =#{teaId }
    </select>
</mapper>